{
  "name": "election-result-upload",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "NODE_ENV=local npm run typeorm -- -d ./src/typeorm/typeorm.config.ts migration:generate ./src/migrations/$npm_config_name",
    "migration-local": "NODE_ENV=local npm run typeorm migration:run -- -d ./src/typeorm/typeorm.config.ts",
    "revert:migration-local": "NODE_ENV=local npm run typeorm -- -d ./src/typeorm/typeorm.config.ts migration:revert",
    "migration-prod": "NODE_ENV=prod npm run typeorm migration:run -- -d ./src/typeorm/typeorm.config.ts",
    "revert:migration-prod": "NODE_ENV=prod npm run typeorm -- -d ./src/typeorm/typeorm.config.ts migration:revert",
    "seed:local": "NODE_ENV=local node dist/seeder/seeder",
    "seed-refresh:local": "NODE_ENV=local node dist/seeder/seeder --refresh",
    "seed": "NODE_ENV=prod node dist/seeder/seeder",
    "seed:refresh": "NODE_ENV=prod node dist/seeder/seeder --refresh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.281.0",
    "@faker-js/faker": "^7.6.0",
    "@forestadmin/agent": "^1.8.4",
    "@forestadmin/datasource-sql": "^1.2.6",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/core": "^7.43.0",
    "@sentry/node": "^7.43.0",
    "@sentry/tracing": "^7.43.0",
    "@types/multer": "^1.4.7",
    "@types/passport-jwt": "^3.0.8",
    "argon2": "^0.30.3",
    "aws-sdk": "^2.1324.0",
    "axios": "^1.3.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mailgen": "^2.0.27",
    "mixpanel": "^0.17.0",
    "nestjs-seeder": "^0.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-magic-login": "^1.2.2",
    "pg": "^8.9.0",
    "randomstring": "^1.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "^4.17.17",
    "@types/jest": "29.4.0",
    "@types/node": "^18.14.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.4.3",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
